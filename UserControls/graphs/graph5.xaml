<UserControl x:Class="projet23_Station_météo_WPF.UserControls.graphs.graph5"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="250" d:DesignWidth="400" Height="250" MinWidth="400" HorizontalAlignment="Left" VerticalAlignment="Top">
    <Grid>
        <Border x:Name="view" BorderBrush="#FFD6D6D6" BorderThickness="0,0,5,5" Margin="0" CornerRadius="20" Background="#FFEEEEEE">
            <Grid>
                <lvc:CartesianChart DisableAnimations="True" x:Name="chart" LegendLocation="Right" Margin="10,11,0,0" >
                </lvc:CartesianChart>
                <Image x:Name="iconBox" HorizontalAlignment="Left" Height="20" Margin="10,10,0,0" VerticalAlignment="Top" Width="20" Source="/projet23-Station météo WPF;component/images/icons/temperature0.png"/>
            </Grid>
        </Border>
    </Grid>
</UserControl>
